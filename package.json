{
	"name": "saturn-nodejs",
	"version": "1.0.0",
	"description": "Saturn node.js in typescript",
	"main": "src/app.ts",
	"scripts": {
		"build": "tsc",
		"heroku-postbuild": "npm run build",
		"start": "node src/app.ts",
		"inspect": "nodemon --inspect src/app.ts",
		"dev": "nodemon src/app.ts",
		"test": "jest",
		"lint": "npm run lint:js ",
		"lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
		"lint:js": "npm run lint:eslint src/",
		"lint:fix": "npm run lint:js -- --fix",
		"generate-documentation": "npx typedoc src/index.ts",
		"git_push": "node -e \"const mssg = ' ' + process.argv[1]; require('child_process').execSync('npx prettier --write . && git add . && git commit -m \\\"' + mssg + '\\\" && git push -u origin master', { stdio:[0, 1, 2] } ) \""
	},
	"keywords": [
		"boilerplay",
		"cron",
		"jobs",
		"js",
		"javascript",
		"typescript",
		"node",
		"express"
	],
	"author": "Iwatannaye Victor",
	"license": "ISC",
	"dependencies": {
		"@sendgrid/mail": "^7.6.0",
		"@useoptic/express-middleware": "^0.0.5",
		"agenda": "^3.1.0",
		"agendash": "^2.1.1",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"cors": "^2.8.5",
		"crypto-js": "^4.1.1",
		"cryptr": "^6.0.2",
		"dotenv": "^8.2.0",
		"errorhandler": "^1.5.1",
		"event-dispatch": "^0.4.1",
		"eventemitter3": "^4.0.7",
		"express": "^4.17.1",
		"express-basic-auth": "^1.2.0",
		"express-jwt": "^6.0.0",
		"form-data": "^2.3.3",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.21",
		"method-override": "^3.0.0",
		"moment": "^2.29.0",
		"moment-timezone": "^0.5.31",
		"mongoose": "^5.10.6",
		"mongoose-aggregate-paginate-v2": "^1.0.6",
		"mongoose-paginate-v2": "^1.7.23",
		"morgan": "^1.10.0",
		"reflect-metadata": "^0.1.13",
		"typedi": "^0.8.0",
		"typedoc": "^0.22.11",
		"validatorjs": "^3.22.1",
		"winston": "^3.3.3"
	},
	"devDependencies": {
		"@types/agenda": "^2.0.9",
		"@types/cryptr": "^4.0.1",
		"@types/express": "^4.17.8",
		"@types/jest": "^26.0.14",
		"@types/lodash": "^4.14.161",
		"@types/mongoose": "^5.7.36",
		"@types/mongoose-aggregate-paginate-v2": "^1.0.9",
		"@types/mongoose-paginate-v2": "^1.6.5",
		"@types/node": "^14.11.2",
		"@typescript-eslint/eslint-plugin": "^4.2.0",
		"@typescript-eslint/parser": "^4.2.0",
		"eslint": "^7.9.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
		"jest": "^26.4.2",
		"nodemon": "^2.0.4",
		"prettier": "^2.1.2",
		"ts-jest": "^26.4.0",
